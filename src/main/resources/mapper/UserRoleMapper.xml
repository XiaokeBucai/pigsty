<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com._220a220e.mapper.UserRoleMapper">
    <resultMap id="BaseResultMap" type="com._220a220e.entity.UserRole">
        <result column="userId" jdbcType="INTEGER" property="user_id" />
        <result column="roleId" jdbcType="INTEGER" property="role_id" />
    </resultMap>

    <delete id="delete" parameterType="map">
        delete from sys_user_role
        where user_id = #{userId,jdbcType=INTEGER}
        and role_id = #{roleId,jdbcType=INTEGER}
    </delete>
    <insert id="insert" parameterType="map">
        insert into sys_user_role (
          user_id,
          role_id
        )
        values (
          #{userId,jdbcType=INTEGER},
          #{roleId,jdbcType=INTEGER}
        )
    </insert>

</mapper>